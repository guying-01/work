@charset "utf-8";
/* CSS Document */
/*订单筛选*/
.order-serch {
	margin: 0 auto;
	padding: 30px 0px 30px 0;
	width: 950px;
}
.order-serch strong {
	display: block;
	margin-left: 20px;
	float: left;
	font-weight: lighter;
	line-height: 25px;
}
.order-serch input {
	float: left;
	width: 130px;
}
.order-serch span {
	display: block;
	padding: 0 5px;
	float: left;
	line-height: 25px;
}
.order-serch select {
	display: block;
	margin-right: 10px;
	float: left;
}
/*全选*/
.allselect {
	margin: 0 auto;
	padding:0 20px 20px 20px;
	clear: both;
	height: 30px;

}
.allselect input {
	display: block;
	margin: 9px 5px 0 0;
	float: left;
}
.allselect span {
	display: block;
	margin-right: 10px;
	float: left;
	line-height: 30px;
}
/*推荐商品*/
.goods {
	display: block;
	margin:25px 0px 0 11px;
	float: left;
	width: 184px;
	height: 275px;
	border: solid 1px #e1e1e1;
}
.goods img {
	display: block;
	margin: 0 auto;
	width: 160px;
	height: 200px;
}
.goods h3 {
	margin: 0 auto 10px auto;
	width: 170px;
	max-height:40px;
	overflow:hidden;
	font-weight: lighter;
	line-height: 20px;
	word-wrap:break-word;
}
.goods:hover .goodsoperation {
	display: block;
}
/*.goodsprice {
	margin: 0 auto;
	width: 170px;
	height: 25px;
}*/
.goodsprice strong {
	/*font-size: 14px;*/
	color: #ca1d3f;
	/*line-height: 25px;*/
}
.goodsprice a {
	line-height: 25px;
}
/*订单详情*/
.payment-page {
	padding-bottom: 45px;
}
/*订单状态*/
.orderdedetail {
	position: relative;
}

.orderdedetail h3 {
	overflow: hidden;
	word-wrap: break-word;
	line-height: 0.5rem;
	font-size: 0.23rem;
	text-indent: 0.08rem;
}
.orderdedetail h3 a:hover {
	color: #ff8b0e
}
.order-status p {
	display: block;
	height: 30px;

	line-height: 30px;
}
.order-status p i {
	padding: 5px;
	background: url(../../img/common/ico3.png) no-repeat 0 -159px;
}
.order-status p span {
	display: block;
	float: left;
}
.order-status p a {
	display: block;
	margin-left: 5px;
	float: left;
	color: #ff8b0e;
}
/*物流信息*/
.logistics-empty {
	margin: 20px auto;
	font-size: 18px;
	color: #ff8b0e;
	text-align: center;
}
.order-logistics li {
	min-height: 30px;
	line-height: 30px;
}
.order-logistics li span, .order-logistics li strong {
	display: block;
	float: left;
	font-weight: lighter;

}
.order-logistics li .left {
	width: 800px;

}
/*订单信息*/
.order-information {
}
.order-information li {
	display: block;
	float: left;
	width: 300px;
	min-height: 30px;

	line-height: 30px;
}
.order-information .float-no {
	clear: both;
	width: auto;
}
/*商品清单*/
.goodslist {
	display: none;
	margin: 0 auto 20px auto;
	width: 900px;
}
.anclose-button {
	height: 15px;
	background: #fff3f3;
}
.anclose-button p {
	display: block;
	margin: 0 auto;
	width: 50px;
	height: 15px;
	background: url(../../img/common/ico01.png) -106px -49px;
	cursor: pointer;
}
.active-anclose-p {
	background: url(../../img/common/ico01.png) -106px -102px !important;
}
.table-tit {
	padding: 7px 0;
	height: 20px;
	border: solid 1px #e1e1e1;
	text-align: center;
	line-height: 20px;
}
.table-tit span {
	display: block;
	float: left;
	width: 95px;
	text-align: center;
}
.order-tab {
	margin: 0 auto;
	border-left: solid 1px #e1e1e1;border-bottom: solid 1px #e1e1e1;
	border-right: solid 1px #e1e1e1;
}
.table-produce {
	padding: 10px 0;
	height: 52px;
	overflow: hidden;
	border-top: solid 1px #e1e1e1;
}
.table-produce p {
	display: block;
	float: left;
	width: 95px;

	text-align: center;
}
.table-produce p .left {
	width: 290px;
	text-align: left !important
}
.table-produce p strong {
	display: block;
	float: left;
	font-weight: lighter;
}
.table-produce p span {
	display: block;
	float: left;
	height: 52px;
	overflow: hidden;
}
.table-produce p span img {
	width: 50px;
	height: 50px;
	border: solid 1px #e1e1e1;
}
.table-message {
	margin: 0 auto;
	width: 900px;
}
.table-message p span {
	display: block;
	width: 90px;

	text-align: center;
	line-height: 30px;
}
.order-sum {
	display: block;
	margin-right: 30px;
	float: right;
	overflow: hidden;
}
.order-sum ul li {
	height: 30px;
	line-height: 30px;
}
.order-sum ul li span{text-align:right;}
.order-sum ul li span, .order-sum ul li strong {
	display: block; font-family: "微软雅黑",Arial;
	float: left;

}
.order-sum ul li .font-color-d {
	font-size: 20px;
}
.display-name {
	height: 30px;
	line-height: 30px;
}
.display-name input {
	margin: 9px 5px 0 0px;
}

