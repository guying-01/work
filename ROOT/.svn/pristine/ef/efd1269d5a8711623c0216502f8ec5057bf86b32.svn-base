    background-size: 100%;
    /*margin-top: -0.07rem;*/
    margin-left: 0.45rem;
}
.footer-tab-item a{color:#333; }
.tab-item3{
	color:#b52137;
}
.footer-tab-item .tab-item3 p{
	color:#b52137;
}
.footer-tab a p {
    font-size: 0.21rem;
}
.footer-tab-item i{
	display: block;
	margin:0 auto 0.02rem auto;
	width: 0.31rem;
	height: 0.32rem;
	
}
.tab-item1 i{
	background: url(../../default/common_img/tab-icon1.png) no-repeat center center;
}
.tab-item2 i{
