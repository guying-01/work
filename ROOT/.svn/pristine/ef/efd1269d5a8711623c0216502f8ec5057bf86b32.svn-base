}
.footer .icon12 li p {
    color: #e68222;
    font-size: 0.18rem;
    line-height: 0.3rem;
}

.footer .about {
    margin: 0 auto;
    padding: 0.2rem 0;
}

.footer .about li {
    float: left;
    width: 33.33%;
    text-align: center;
}

.footer .about li a {
    display: inline-block;
    font-size: 0.22rem;
    color: #979797;
    line-height: 0.4rem;
    border: 1px solid #979797;
    border-radius: 0.3rem;
    padding: 0 0.15rem;
    margin: 0.1rem 0;
}

.footer .tool_box {
    width: 100%;
    background: #fddf03;
    font-size: 0.2rem;
    overflow: hidden;
    padding-bottom:0.16rem;
}
.footer .tool{
    padding: 0.2rem 0.05rem 0.1rem;
    width: 100%;
    box-sizing: border-box;
    text-align: center;
}
.footer .tool li{
    float: left;
    width: 25%;
    box-sizing: border-box;
    margin-bottom: 0.06rem;
    text-align: center;
}
.footer .tool li a{
    color: #000;
    font-size: 0.22rem;
    width: 100%;
    padding: 0.05rem 0;
}
.footer .tool_b_l{
    float: left;
    padding-left: 0.44rem;
    margin-right: 0.5rem;
}
.footer .tool_b_l .logo{
    width:1.2rem;
    height:0.46rem;
    color:#000 ;
    font-weight: bold;
}
.footer .tool_b_l .logo img{
    width: 100%;
    height: 100%;
}
.footer .tool_b_l p{
    padding-top: 0.1rem;
}
.footer .tool_b_site{
    float: left;
    width: 3.22rem;
    height: 0.43rem;
    line-height: 0.43rem;
    border-radius:0.16rem;
    text-align: center;
    border:1px solid #000;
    font-size: 0.22rem;
}
.footer .tool_serial_num{
    float: left;
    width: 3.22rem;
    text-align: center;
    line-height: 0.4rem;
    padding-top: 0.05rem;
}
.footer .two_code_box{
    width: 100%;
    background:#56c9ff;
    padding-top: 0.3rem;
    overflow: hidden;
}
.footer .two_code{
    margin:0 auto;
    line-height: 0.4rem;
}
.footer .two_code li{
    float: left;
    width: 33.33%;
    font-size:0.2rem;
    color:#fff;
    text-align: center;
    margin-bottom: 0.24rem;
}
.footer .two_code li{
    float: left;
}
.footer .two_code li img{
    width: 0.9rem;
    height: 0.9rem;
}
/*----------footer结束-----------*/
/*底部tab开始*/
.footer-tab{
	width: 100%;
	display: block;
    height: 0.87rem;
	position: fixed;
	left: 0;
	bottom: 0;
	background: #fff;
	border-top: 1px solid #E7E7E7;
    border-bottom: 1px solid #F8F8F8;
    z-index: 1000;
}
.footer-tab-item{
	width: 20%;
	height: 100%;
	text-align: center;
	color:#696969 ;
	float: left;
	padding-top:0.15rem;
	font-size:0.14rem;
	line-height: 1.5;
}
.tab-item1.on i{
    background: url(/resources/basepage/store/default/common_img/tab-icon1R.png) no-repeat center center;
    background-size: 100%;
}
.tab-item1.on{
    color: #b52137;
}
.footer-tab-item.tab-item3 i{
	width: 0.6rem;
