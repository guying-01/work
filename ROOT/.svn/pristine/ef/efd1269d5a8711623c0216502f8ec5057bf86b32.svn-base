html,
body {
    height: 100%;
    font-size: 0.25rem;
    font-family: "microsoft yahei";
    width: 100%;
    -webkit-text-size-adjust: 100% !important;
    /*微信ios禁止用户放大*/
}

html,
body,
div,
ul,
li,
p,
a,
h4,
img,
dl,
dt,
dd {
    margin: 0;
    padding: 0;
}

li {
    list-style: none;
}

a {
    text-decoration: none;
}

a:hover {
    text-decoration: none;
}
.my_clear:after{
    content: "";
    display: block;
    clear: both;
}
/*头部导航header_top开始*/
#header_top{width: 100%;overflow: hidden}
.header_box{width: 100%;background: #f5f5f5;}
.header_box ul{display: block;height: 0.5rem;min-width: 6.6rem;overflow: hidden;}
.header_box ul li{float: left;height: 0.5rem;line-height: 0.5rem;font-size: 0.24rem;padding: 0 0.1rem;}
.header_box ul li:last-child{padding-right: 0;}
.header_box ul li a{display: block;margin-left: 0.05rem;color: black;}
@media only screen and (max-width: 320px) {
    .header_box ul li{font-size: 0.22rem;}
    .header_box ul li a{margin-left: 0.05rem;}

}
/*头部导航header_top结束*/
.top_banner{width:6.4rem;height: 3rem;overflow: hidden;position: relative;}
.top_banner>img{height: 100%;width: 100%;}
/*search_box*/

/*mobile part start*/
.hiddenSearchBox{
    width: 100%;
    height: 15rem;
    background: #fff;
    z-index: 999;
    position: relative;
    border-top:1px solid #000;
    left:0;
    top:0;
}
.hiddenSearchBox .searchBar{
    width: 100%;
    height: 0.5rem;
    border-bottom:1px solid #000;
    background: #efefef;
    box-sizing: border-box;
    padding: 0 0.2rem;
    position: relative;
    top:0;
    left:0;
}
.hiddenSearchBox .searchClose{
    width: 0.4rem;
    height: 0.4rem;
    text-align: center;
    line-height: 0.4rem;
    margin-top: 0.05rem;
    font-size: 0.5rem;
    color: #666;
    float: left;
}
.hiddenSearchBox .searchContent{
    width: 85%;
    border-radius: 0.1rem;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
    float: left;
    height: 0.4rem;
    box-sizing: border-box;
    border: 1px solid #000;
    padding-left: 0.1rem;
}
.hiddenSearchBox .searchBar .searchBox{

}
.hiddenSearchBox .searchBar  .btn{
    position: absolute;
    right:0.5rem;
    top:0;
    height: 0.4rem;
    margin-top: 0.05rem;
    line-height: 0.4rem;
    border-left: 1px solid #000;
    text-align: center;
    width: 0.6rem;
    z-index: 1000;
}
.hiddenSearchBox .recentSearch{
    width: 100%;
    height: auto;
    over-flow:hidden;
    padding: 0 0.2rem;
    box-sizing: border-box;
}
.hiddenSearchBox .recentSearch .goods li{
    width: 30%;
    height: 0.4rem;
    border-radius: 0.2rem;
    border:0.03rem solid #000;
    border-radius: 0.2rem;
    float: left;
    line-height: 0.4rem;
    box-sizing: border-box;
    padding-left: 0.1rem;
}
.hiddenSearchBox .hotSearch{
    width: 100%;
    height: auto;
    over-flow:hidden;
    padding: 0 0.2rem;
    box-sizing: border-box;
}
.hiddenSearchBox .hotSearch ul li{
    width: 30%;
    height: 0.4rem;
    border:0.03rem solid #000;
    border-radius: 0.2rem;
    float: left;
    line-height: 0.4rem;
    box-sizing: border-box;
    padding-left: 0.1rem;
    margin-top: 0.1rem;
    margin-right:3%;
}
/*mobile part end*/
.top{width: 100%;overflow: hidden;}
.search_box{position: absolute;top:0;width: 6.4rem;background: rgba(255,255,255,0.6);font-size: 0.24rem;line-height: 0.45rem;height: 0.73rem;text-align: center;}
.search_box div{display: block;float: left;color: #434343;}

.search_box .login_link{
    border: 0.001rem solid #999;
    border-radius:0.1rem;
    background: #fff;
    height: 0.45rem;
    margin-top: 0.125rem;
    padding: 0 0.1rem;
    box-sizing: border-box;
    /*width:1.5rem;*/
    text-align: center;
    float: left;
    margin-left: 0.1rem;
    visibility: hidden;
}
.category_link{margin-left: 3%;}
.search_link{
    width: 2.9rem;background: #fff;border-radius: 0.4rem;height: 0.5rem;margin-top: 0.1rem;border: 0.001rem solid #999;font-size: 0.2rem;
    box-sizing: border-box;padding:0 0.1rem;line-height: normal;text-align: center;outline: none;color: #434343;font-family: inherit;}
.search_box .btn i{
	display: block;
	width:0.3rem;
	height: 0.3rem;
	background: url(/resources/basepage/store/default/common_img/search_box_btn.png) no-repeat center center;
	background-size: 100%;
	float: left;
	margin:0.07rem 0.1rem 0 0.1rem;
}
.search_box .btn span{
	float: left;
	color: #434343;
	letter-spacing: 0.02rem;
}
.search_box .btn{
    border: 0.001rem solid #999;
    border-radius: 0.1rem;
    background: #fff;
    height: 0.45rem;
    margin-top: 0.125rem;
    padding: 0 0.1rem;
    box-sizing: border-box;
    width: 1.5rem;
    float: right;
    margin-right: 0.1rem;
    cursor: pointer;
}
.search_link img{height: 94%;padding-left: 0.15rem;margin-top: 0.02rem;}
.search_box .gohome{
    height: 0.45rem;
    line-height: 0.45rem;
    padding: 0 0.1rem;
    border: 1px solid #999;
    margin-top: 0.125rem;
    border-radius: 0.1rem;
    box-sizing: border-box;
    margin-left: 0.1rem;
    background: #fff;
}
.search_box .b_wid{
    width: 1.5rem;
}
/*-------footer开始---------*/
.footer .icon12:after,
.footer .about:after,
.tool .about:after,.tool:after,.two_code:after {
