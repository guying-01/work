}
.tab-item5 i{
	background: url(../../default/common_img/tab-icon5.jpg) no-repeat center center;
}
.footer-tab-item i{
	background-size: 100%;
}

/*mobile part start*/
@media screen and (max-width: 768px){
    .footer .tool li a  {
        font-size: 0.2rem;
        float: left;
    }
