/*登录弹出框的小螃蟹*/
.login_i{
    position: absolute;
    z-index: -1;
    top: -1rem;
    left: 1rem;
}
.login_i img{
    width: 3rem;
}
.ff_header_bar{
    height: 0.8rem;
    line-height: 0.8rem;
    padding: 0 0.1rem;
    position: relative;
    box-sizing: border-box;
    border-top: 1px solid #999;
}
.common_header_back{
    width: 1.2rem;
    height: 0.8rem;
    line-height: 0.8rem;
    position: absolute;
    z-index: 1;
    left: 0.15rem;
    top:0;
    cursor: pointer;
}
.common_header_back:after{
    content: '';
    display: block;
    clear: both;
}
.common_header_back i{
    background: url("../common_img/go_back_icon.png") no-repeat center left;
    background-size: contain;
    display: block;
    float: left;
    top: 50%;
    width: 0.8rem;
    height: 0.45rem;
    margin-top: 0.22rem;
    font-style: normal;
    color: #b1191a;
    text-align: right;
    line-height: 0.45rem;
}
.common_header_back span{
    padding-left: 0.1rem;
    color: #b1191a;
    letter-spacing: 2px;
    line-height: 0.85rem;
    font-size: 0.26rem;
}
.ff_header_bar .header_title{
    font-size: 0.32rem;
    line-height: 0.8rem;
    text-align: center;
}
.ff_header_bar .tool-bt{
    position: absolute;
    z-index: 1;
    right: 0.1rem;
    top: 0;
    width: 1.3rem;
    height: 0.8rem;
    line-height: 0.8rem;
    cursor: pointer;
}
.ff_list_search_box {
    height: 0.73rem;
    position: relative;
}