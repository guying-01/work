    border: solid #e5e5e5;
    border-width: 1px 0;
}

.footer .icon12 {

    padding: 0.3rem 0.1rem 0.1rem;
}

.footer .icon12 li {
    float: left;
    width: 25%;
    text-align: center;
    margin-bottom: 0.25rem;
}

.footer .icon12 li i {
    display: block;
    width: 0.5rem;
    height: 0.5rem;
    margin: 0 auto;
