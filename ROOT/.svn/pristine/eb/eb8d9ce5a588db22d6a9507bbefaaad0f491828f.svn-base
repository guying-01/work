<#--共通标题返回按钮-->
<#macro render title,toolName>
<div class="ff_header_bar">
	<div class="common_header_back" id="common_header_back" onclick="javascript :history.back();">
		<i></i><span>返回</span>
	</div>
	<div class="header_title">${title}</div>
	<div class="tool-bt" id="header_bar_tbt">
		<span>${toolName}</span>
	</div>
</div>
</#macro>