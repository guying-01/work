.recommend_box .t_365{border-top: 1px solid #dcdcdc;}
.recommend_box .creazy{width: 3.8rem;height: 1.67rem;border-top: 1px solid #dcdcdc;}
.recommend_box .sales,.recommend_box .top{width: 1.89rem;height: 1.67rem;}
.recommend_box .sales{border-right: 1px solid #dcdcdc;}
/*天上掉馅饼-热卖榜单-新奇特怪推荐结束*/
/*8icon开始*/
.e_icon_box{width: 100%;overflow: hidden;color: #eb4f38;}
