/*猜你喜欢开始*/
.page1_title{width: 100%;height: 0.56rem;font-size: 0.28rem;line-height: 0.56rem;text-align: center;margin: 15px 0;}
.page1_title img{height: 0.27rem;vertical-align: middle;padding: 0 1%;margin-top: -0.02rem}
.maylike_box{width: 100%;overflow: hidden;}
.maylike_box .page1_title{background: #c91622;color: #fff;}
.maylike_box .maylike{width: 96%;margin: auto;}
.maylike_box .maylike>div{width: 1.96rem;height: 3rem;overflow: hidden;margin-right: 2%;float: left;}
.maylike_box .maylike>div:last-child{margin-right: 0;}
.maylike_box .maylike>div>a{display: block;width: 100%;height: 1.817rem;}
.maylike_box .maylike>div>a>img{width: 98%;height:98%;border: 1px solid #dcdcdc;margin: auto;}
.maylike_box .maylike>div>div>a>div{max-height: 0.6rem;overflow: hidden;display: block;/*white-space:nowrap;*/text-overflow:ellipsis;}
.maylike_box .maylike>div>div{width: 100%;font-size: 0.26rem;text-align: center;line-height: 0.3rem;}
.maylike_box .maylike>div>div>a{display: block;padding-top: 0.1rem;}
.maylike_box .maylike>div>div>a>p{font-size: 0.26rem;color: #b5191a;text-align: center;margin-top: 0.1rem;word-break: break-all;overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;}
/*猜你喜欢结束*/
/*订阅商机开始*/
.opportunity_box{width: 100%;overflow: hidden;text-align: center;}
