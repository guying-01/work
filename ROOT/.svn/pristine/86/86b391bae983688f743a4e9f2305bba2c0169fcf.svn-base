.brand_main_box .title{color: #333;height: 0.6rem;line-height: 0.6rem;font-size: 0.3rem;/*padding-left: 0.3rem;*/border-bottom: 0.05rem solid #333;text-align: center;}
.brand_main_box .item_list{padding-top: 0.3rem;background: #f5f5f5;padding-bottom: 0.3rem;}
.brand_main_box .item_list .item{display: block;width: 6rem;margin:0 auto 0.3rem;border: 1px solid #d6d6da;border-radius: 0.1rem;background: #fff;}
.brand_main_box .item_list .item:last-child{margin-bottom: 0;}
.brand_main_box .item_list .item a{color: inherit;}
.brand_main_box .item_list .item .brand_name{font-size: 0.24rem;line-height: 0.3rem;height: 0.3rem;padding: 0.15rem 0 0.05rem 0.2rem;}
.brand_main_box .item_list .item .in_box{width: 5.6rem;margin: auto;padding: 0.1rem 0 0.2rem;border-bottom: 1px solid #d6d6da;}
.brand_main_box .item_list .item .in_box div{float: left;padding-left: 0.15rem;width: 3.8rem;padding-top: 0.1rem;line-height: 0.3rem;}
.brand_main_box .item_list .item .in_box .long{padding-top: 0;}
.brand_main_box .item_list .item img{width: 1.55rem;height: 0.8rem;display: block;float: left;border: 1px solid #d6d6da;border-radius: 0.1rem;}
.brand_main_box .item_list .item p{display: block;height: 0.3rem;line-height: 0.3rem;padding: 0.1rem 0;text-align: center;color: #c91622;}
/*品牌厂家结束*/
/*产地直供特殊样式开始*/
.chandi_main_box .category_list{padding:0 0.15rem 0.2rem; }
.chandi_main_box .category_list a{color: #333;}
.chandi_main_box .category_list li{float: left;min-width: 0.1rem;height: 0.4rem;padding: 0 0.2rem;color: #333;margin-left: 0.1rem;margin-top:0.2rem;line-height: 0.4rem;border-radius: 0.1rem;font-size: 0.24rem;border: 1px solid #333;}
.chandi_main_box .item_list{display: none;}
.chandi_main_box .category_list .on {background:#c91622;color: #fff;border-color:#c91622; }
.chandi_main_box .on a{color: #fff;}
/*产地直供结束*/
/*酒店装修特殊样式开始*/
.h_main_box .item_list{display: block;}
/*酒店装修结束*/