$(function () {
   $('#static_box').on('click', '.tab li', function () {
       var index = $(this).index();
       $(this).addClass('on').siblings().removeClass('on');
       $('.tab' + index).show().siblings('div').hide();
   });
   $('#static_box').on('click', '.divTab li', function () {
      var indexD = $(this).index();
      $(this).addClass('on').siblings().removeClass('on');
      $(this).parent().siblings('.style' + indexD).show().siblings('div').hide();
   })

});
