@charset "utf-8";
/**购物车**/

.cart_header {
	height: 0.8rem;
	background: white;
	position: relative;
	border-bottom: 0.01rem solid #e1e1e1;
}
.cart_header span {
	display: inline-block;
	font-weight: bold;
	font-size: 0.35rem;
	margin-left: 2.6rem;
	line-height: 0.8rem;
}
.cart_header .edit {
	position: absolute;
  font-size: 0.25rem;
  background: #db0401;
  width: 0.7rem;
  text-align: center;
  border-radius: 0.1rem;
  line-height: 0.5rem;
  top: 0.15rem;
  margin: 0;
  right: 0.1rem;
  color: white;
  font-weight: normal;
}
.cart_header a {
	position: absolute;
	display: inline-block;
	height: 0.8rem;
	width: 0.35rem;
	left: 0.1rem;
	top: 0.05rem;
	background: url(../../img/member/goBack.png) no-repeat center center;
	background-size: contain;
}

.car-logo {
	position: absolute;
	top: 30px;
	left: 210px;
	width: 85px;
	height: 50px;
	font-size: 16px;
	text-align: center;
	line-height: 43px;
	border-left: solid 1px #6c6c6c;
}
.search {
	left: 290px;
	width: 405px;
}
.search input {
	width: 315px;
}
.search p {
	width: 315px;
}
.hot-search {
	left: 295px;
	width: 380px;
}
.cartable-tr {
	height: 45px;
	border: solid 1px #ececec;
	background: #fff;
}
.cartable-tr p {
	display: block;
	float: left;
	width: 119px;
	text-align: center;
	line-height: 45px;
}
.cartable {
	border-left: solid 1px #ececec;
	border-right: solid 1px #ececec;
	margin-bottom: 0.15rem;
}
.car-message {
	height: 0.65rem;
	border: solid 1px #ececec;
	background: #fff;
	line-height: 0.65rem;
}
.car-message .m-l-10 {
	color: #999;
	font-size: 0.24rem;
}

.car-message a {
	color: #db0401;
}
.car-message input, .car-message .consulting {
	display: block;
	color: #db0401;
  float: right;
  margin-top: 0.15rem;
  margin-right: 0.2rem;
}
.car-message input {
	margin: 0.21rem 0 0 0.15rem;
}
.cartable-td {
	clear: both;
	padding: 0.1rem 0;
	min-height: 60px;
	border-bottom: solid 1px #ececec;
	line-height: 0.27rem;
}
.forCombinacation {
    margin-top: 0.1rem;
    float: right;
    margin-right: 0.1rem;

}
.cartable-td .left {
	/*position: relative;*/
	width: 1.3rem;
}
.cartable-td .checkBtns {
	width: 0.6rem;
	padding-top: 0.3rem;
	padding-left: 0.19rem;
}
.cartable-td .goodsImg {
	width: 1.1rem;
	padding-top: 0.05rem;
}
.cartable-td .textalign-l a{
	display: block;
  font-size: 0.25rem;
  color: black;
  padding-top: 0.05rem;

}
.cartable-td .left strong {
	font-size: 18px;
	color: #b32d10;
	font-family: Arial, "微软雅黑";
}
.cartable-td .left a:hover {
	color: #db0401;
}
.cartable-td .left u {
	display: block;
	text-decoration: line-through;
}
.cartable-td .left .errorwrong, .cartable-td .left .errorcorrect {
	bottom: 59px;
	left: 10px;
	white-space: nowrap;
}
.cartable-td div img {
	width: 1.1rem;
  height: 1.1rem;
}
.failure {
	display: inline-block;
	padding: 0 3px;
	border: solid 1px #d0d0d0;
	background: #efefef;
	font-size: 12px;
	color: #7e7e7e;
	text-align: center;
	line-height: 20px !important
}
.gray div, .gray div strong, .gray div input {
	color: #bbb !important;
}
.gray a {
	cursor: default;
}
.produce {
}
.bottomWidthA {
	height: 0.7rem;
	margin-top: 10px;
}
.bottomWidth {
	z-index: 10;
}
.bottom-nav {
	clear: both;
	height: 1rem;
	margin: 0 auto;
	border: solid 1px #ececec;
	background: #fff;
	position: fixed;
	bottom: 0.85rem;
}
.bottom-navB {
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
}
.bottom_left {
    position: fixed;
    bottom: 1.71rem;
    width: 100%;
    background: #fff;
    border-top: 0.05rem solid #eee;
    padding-bottom: 0.06rem;
    display: none;
    border-bottom: 1px solid #ddd;
		display: none;
}

.bottom-nav .left input {
	display: block;
	margin: 23px 5px 0 20px;
	float: left;
}
.bottom-nav .left a, .bottom-nav .left p {
	float: left;
	line-height: 0.6rem;
}
.bottom-nav .left p {
	margin-right: 0.15rem;
	margin-top: 0.2rem;
}
.bottom-nav .left a {
	margin-left: 0.15rem;
	display: block;
	background: #f88c0e;
	margin-top: 0.15rem;
	padding: 0 0.1rem;
	color: white;
	font-size: 0.22rem;
	margin-bottom: 0.1rem;
}
.bottom-nav .payRight  {
    position: fixed;
    bottom: 0.86rem;
    width: 100%;
    height: 0.85rem;
	padding-top: 0.05rem;
	background: #eee;
	border-top: 1px solid #ddd;
}
.bottom-nav .right .total_goods {
    float: right;
}
.bottom-nav .payRight .allCheck {
    float: left;
    padding-left: 0.2rem;
    padding-top: 0.3rem;
}
.bottom-nav .payRight .allCheck p {
    float: right;
}

.bottom-nav .payRight .carPrice {
    text-align: right;
    line-height: 0.3rem;
    float: left;
    margin-left: 0.4rem;
    margin-top: 0.1rem;
}
.bottom-nav .payRight .carPrice p {
    display: inline-block;
		font-size: 0.24rem;
}
.bottom-nav .payRight .carPrice strong {
	width: 2.2rem;
	display: inline-block;
	vertical-align: middle;
	text-align: left;
}
.bottom-nav .payRight .carPrice span {
    line-height: 0.4rem;
    padding-right: 0.33rem;
    display: block;
}


.bottom-nav .carPrice strong {
	color: #b32d10;
    font-size: 0.25rem;
	font-family: Arial, "微软雅黑";
}
.bottom-nav .carPrice b {
	font-family: Arial, "微软雅黑";
	font-size: 0.27rem;
	padding: 0 0.1rem;
}
.bottom-nav b {
	color: #b32d10;
}
.produce .tabT {
	position: relative;
	display: block;
	height: 31px;
	border: solid 1px #ddd;
	border-top: 0;
	border-right: 0;
}
.produce .tabT li {
	position: relative;
	display: block;
	float: left;
	width: 111px;
	height: 31px;
}
.produce .tabT li span {
	position: absolute;
	display: block;
	top: 0;
	left: 0;
	width: 110px;
	height: 30px;
	border-right: solid 1px #ddd;
	border-top: solid 1px #ddd;
	background: #f5f5f5;
	text-align: center;
	line-height: 30px;
	cursor: pointer;
}
.produce .tabT .on span {
	z-index: 100;
	top: -5px !important;
	left: -1px !important;
	padding-top: 5px;
	border-bottom: solid 1px #fff;
	border-top: solid 2px #0dab88 !important;
	border-left: solid 1px #ddd;
	border-left: solid 1px #ddd;
	background: #fff !important;
	font-weight: bold;
	color: #0dab88 !important;
}
.taglist {
	display: block;
	margin-top: 20px;
	padding: 0 20px;
	height: 30px;
	background: #eff4f3;
}
.produce .tabT li span:hover {
	color: #0dab88;
}
/*确认支付*/
.payment-page {
	padding-bottom: 50px;
}
.payment-page h2 {
	padding: 10px 0;
	font-size: 20px;
}
.payment-page h2 p {
	font-size: 14px;
	font-weight: lighter;
	line-height: 30px;
}
.payment-page h2 a {
	float: right;
	font-size: 12px;
	font-weight: lighter;
	color: #0dab88;
}
.order {
	padding-top: 30px;
}
.order-adress {
	padding: 20px 0;
}
.order-adress li {
	position: relative;
	display: block;
	clear: both;
	margin-bottom: 0.2rem;
}
.order-adress-list:after,.order-adress-selected:after{
	content: '.';
	clear: both;
	visibility: hidden;
	height: 0;
	display: block;
}
.order-adress-list span{
	display: block;
	width: 5.2rem;
	float: left;
	word-break: break-all;
}
.order-adress-list input {
	display: block;
	float: left;
	margin-top: 0.04rem;
}
.order-adress-con{
	width:100%;
	padding:0.1rem;
	box-sizing: border-box;
	word-break: break-all;
}
.order-adress-list span {
	display: block;
	float: left;
	font-size:0.22rem;
	margin-left: 0.2rem;
}
.order-adress-list span p{
	width: 100%;
	word-break: break-all;
	margin-bottom: 0.15rem;
	font-size: 0.24rem;
}
.order-adress-list span p b{
	font-size: 0.24rem;
}
.order-adress-list a {
	padding-left: 20px;
	color: #0dab88;
}
.order-adress-selected {
	clear: both;
	width:6rem;
	box-sizing: border-box;
	margin:0 auto;
	border: 1px solid #f50;
	background: #fff0e8;
	box-sizing: border-box;
}
.order-adress-list{
    clear: both;
    width: 6rem;
    box-sizing: border-box;
    margin: 0 auto;
    border: 1px solid #dcdcdc;
    background: #f8f8f8;
    box-sizing: border-box;
    padding:0.1rem 0 0.1rem 0.1rem;
}
.order-adress-selected .order-adress-list{
	 width: 5.9rem;
	 background: none;
	 border:none;
	 /*padding:0.05rem 0 0 0.1rem;*/
}
.order-adress-selected h3{
	text-align: center;
	height: 0.6rem;
	line-height: 0.6rem;
	border-bottom: 1px solid #f50;
	font-size: 0.3rem;
}
.order-adress-list p {
	display: block;
	width:100%;
	float: left;
	font-size: 0.26rem;
}
.order-adress-selected a {
	display: block;
	margin-right: 20px;
	float: right;
	color: #0dab88;
	line-height: 30px;
}
.order-tab {
	margin: 0 auto;
	width: 1100px;
	border-left: solid 1px #ddd;
	border-right: solid 1px #ddd;
}
.member-list {
	clear: both;
}
.member-list .tabT {
	height: 31px;
	border-bottom: solid 1px #f2f2f2;
	margin-top: 2px;
}
.member-list .tabT li {
	position: relative;
	display: block;
	float: left;
	width: 131px;
	height: 31px;
}
.member-list .tabT li span {
	position: absolute;
	top: 0;
	left: 0;
	display: block;
	width: 130px;
	height: 30px;
	border-right: solid 1px #f2f2f2;
	border-top: solid 1px #f2f2f2;
	background: #fff;
	font-size: 14px;
	text-align: center;
	line-height: 30px;
	cursor: pointer;
}
.member-list .tabT .on span {
	top: -2px !important;
	left: -1px !important;
	height: 32px;
	border-bottom: solid 1px #fff;
	border-top: solid 2px #db0401 !important;
	border-left: solid 1px #f2f2f2;
	border-left: solid 1px #f2f2f2;
}
.member-list .tabT li span:hover {
}
.member-list-tab {
	padding-bottom: 30px;
	clear: both;
	background: #fff;
}
/*推荐商品*/
.goods {
	display: block;
	margin: 15px 0 0 22px;
	float: left;
	width: 173px;
	height: 255px;
	border: solid 1px #ececec;
}
.goods img {
	display: block;
	margin: 12px auto 0 auto;
	width: 150px;
	height: 150px;
}
.goods h3 {
	margin: 10px auto 5px auto;
	width: 150px;
	height: 40px;
	overflow: hidden;
	font-weight: lighter;
	line-height: 20px;
	word-wrap: break-word;
}
.goods h3 a:hover {
	color: #db0401;
}
.goodsprice {
	margin: 0 auto;
	width: 150px;
	height: 25px;
}
.goodsprice strong {
	font-size: 14px;
	color: #ca1d3f;
	font-family: Arial, Helvetica, sans-serif;
	line-height: 25px;
}
.goodsprice strong b {
	color: #ca1d3f;
	font-family: "微软雅黑";
}
.goodsprice a {
	color: #f88c0e;
	line-height: 25px;
}
/*数据空*/
.nullC {
  height: 1rem;
	margin-right: 0.8rem;
	float: right;
}
.nullC div.left {
	margin: 10px 0 0 15px;
}
.nullC img {
	margin-right: 0.15rem;
	height: 0.8rem;
	width: 0.8rem;
	margin-top: 0.1rem;
}
.nullC .null {
	float: right;
}
.nullC .null p {
  line-height: 0.47rem;
	font-size: 0.24rem;
}
.nullC .null p a {
	color: #bf1b24;
}

.nullC div.left p {
	line-height: 0.25rem;
	font-size: 0.24rem;
}
.nullC div.left p a {
	color: #db0401;
	text-decoration: underline;
}
.nullA {
  height: 1rem;
	margin: 0 auto;
}
.nullA div.left {
	width: 270px;
	margin: 10px 0 0 15px;
}
.nullA div.left p {
	line-height: 20px;
	font-size: 14px;
}
.nullA div.left p a {
	color: #db0401;
	text-decoration: underline;
}
.carNulla {
	padding: 0.7rem 0;
	background: #fff;
	overflow: hidden;
}
/*new css by wu*/
.wrapper {
	width: 100%;
}
.nullGoods {
    height: 4rem;
}
@media screen and (max-width:768px){
	.cart_header span{
		font-size: 0.35rem;
	}
	.cart_header .edit{
		font-size: 0.23rem;
	}
	label .deletegoods{
		border: 0.01rem solid #ddd;
		padding: 0.05rem 0.1rem;
		background: #b32d10;
		color: #fff;
	}
	.delBox{
		float: right;
		margin-right: 0.7rem;
	}
	.cart_header a{
		height: 0.4rem;
		top:50%;
		margin-top: -0.2rem;
	}
}
@media screen and (max-width:320px) {
    .delGoodsBtn {
        margin-left: 2.7rem !important;
    }
    .bottom-nav .right .total_goods {
        margin-right: 1.14rem;
    }
	.delBox{
		margin-right: 0.6rem;
	}
}
#checkBoxes {
	opacity: 0;
}

.checkBoxes {
	background: url("../../img/cart/unChecked.png") no-repeat center center;
	height: 0.25rem;
	width: 0.25rem;
	display: inline-block;
	background-size: 100%;
	margin-top: 0.03rem;
	margin-right: 0.03rem;
}
.isChecked {
	background: url("../../img/cart/checked.png") no-repeat center center;
	background-size: cover;
}
#shopcart_list .shopCheck {
	float: left;
	margin-top: 0.2rem;
	margin-left: 0.18rem;
}
