@charset "utf-8";
/* CSS Document */
body {
	background: #f5f5f5;
}
.banner {
	position: relative;
	z-index: 0;
	width: 100%;
	height: 370px;
	overflow: hidden;
}
.min-banner {
	width: 100%;
	height: 370px;
}
.min-banner li {
	display: none;
	width: 100%;
	height: 370px;
	text-align: center;
}
.sliderBtn {
	background: url(../../img/index/sprite.png) no-repeat;
	cursor: pointer;
	height: 60px;
	position: absolute;
	top: 150px;
	left: 50%;
	z-index: 9;
	width: 0;
}
.prev {
	margin-left: -385px;
}
.next {
	background-position: right top;
	margin-left: 385px;
}
.banner_num {
	position: absolute;
left:50%;
	/*right: 230px !important;*/
	bottom: 10px;
	/*width: 378px !important;*/
	height: 12px;
	padding: 5px 7px;
	background: url(../../img/common/pngB.png) repeat;
	text-align: center;
	border-radius: 10px;
}
.banner_num li {
	float: left;
	width: 12px;
	height: 12px;
	margin: 0 3px;
	cursor: pointer;
	overflow: hidden;
	background: url(../../img/index/roundness.png) 0 0 no-repeat;
}
.banner_num li:hover, .btnListOn {
	background: url(../../img/index/roundness.png) 0 -13px no-repeat !important;
}
.bannerUpAd {
	width: 1200px;
	position: relative;
	height: 0;
	margin: 0 auto;
}
.bannerUpAdImg {
	position: absolute;
	top: -370px;
	right: 0;
	width: 165px;
	height: 370px;
}
.bannerUpAdImg a, .bannerUpAdImg img {
	display: block;
	width: 165px;
	height: 185px;
}
/**/
.background01 {
	background: #ff8607;
}
.background02 {
	background: #fd7599;
}
.background03 {
	background: #2497f0;
}
.background04 {
	background: #eb2972;
}
.background05 {
	background: #eb5013;
}
.active-tit {
	position: relative;
	height: 90px;
	line-height: 90px;
	text-align: center;
}
.active-tit img {
	vertical-align: middle;
}
.active-tit a {
	position: absolute;
	right: 0;
	bottom: 0;
	line-height: 40px;
	font-size: 14px;
	color: #999;
	font-weight: lighter;
}
.s-prod-four {
	margin-left: 7px;
}
.s-prod-four li {
	position: relative;
	display: inline-block;
	width: 198px;
	margin: 0 auto;
	margin-left: -5px;
	margin-top: -1px;
	height: 275px;
	background-color: #fff;
	border: 1px solid #e3e3e3;
	vertical-align: top;
}
.prod-img {
	padding: 5px;
	width: 188px;
	height: 163px;
	border-bottom: 1px solid #e3e3e3;
	display: block;
}
.prod-img img {
	width: 188px;
	height: 163px;
}
.s-prod-four li:hover .prod-img {
	opacity: .85;
}
.prod-text {
	width: 175px;
	margin: 0 auto;
}
.prod-text h3 {
	width: 175px;
	height: 20px;
	margin-top: 10px;
	line-height: 20px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	font-weight: lighter;
}
.price {
	height: 20px;
	line-height: 20px;
	font-size: 18px;
	color: #ca1d3f;
	font-weight: bold;
	font-family: Arial, Helvetica, sans-serif;
}
.price strong {
	color: #ca1d3f;
	vertical-align: middle;
}
.price-u {
	height: 20px;
	line-height: 20px;
}
.price-u u {
	float: left;
	text-decoration: line-through;
	color: #999;
}
.price-u span {
	float: right;
	color: #666;
}
.prod-shop {
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 25px;
	line-height: 25px;
	text-align: center;
	color: #fff;
}
.prod-shop img {
	vertical-align: middle;
	margin-left: 5px;
}
.prod-sale {
	position: absolute;
	top: 152px;
	left: 50%;
	width: 171px;
	height: 30px;
	margin-left: -85px;
	background: #fff;
	border: solid 1px #e1e1e1;
	box-shadow: 0 0 8px 0 rgba(0, 0, 0, 0.26);
	border-radius: 15px;
	font-size: 12px;
	color: #b10200;
	line-height: 30px;
	text-align: center;
}
.active-tab {
	height: 50px;
	background: #bd0200;
	border: solid 1px #fcd8d8;
	border-right: 0;
}
.active-tab li {
	position: relative;
	width: 298px;
	float: left;
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 16px;
	color: #4e0100;
	cursor: pointer;
	border-right: solid 1px #a80301;
}
.active-tab li i {
	display: none;
}
.active-tab .on {
	background: #df0301;
	width: 299px;
	color: #fffed1;
}
.active-tab .on i {
	display: block;
	position: absolute;
	bottom: -1px;
	left: 50%;
	margin-left: -8px;
	width: 0;
	height: 0;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 8px solid #fff;
}
.active-time {
	line-height: 20px;
	padding-top: 5px;
	font-size: 16px;
	color: #fffed1;
}
.active-minute {
	line-height: 20px;
	font-size: 14px;
	color: #fced53;
}
.active-minute span {
	color: #fff;
	margin-right: 5px;
}
/*活动工具条*/	
.active-tool {
	position: fixed;
	bottom: 70px;
	right: 10px;
	width: 100px;
	padding-bottom:31px;
	}
.active-tool-top {
	border: solid 1px #cb0200;
	border-bottom: 0;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	background: #ff0037;
	overflow: hidden;
}
.active-tool-top a {
	display: block;
	line-height: 29px;
	height: 29px;
	border-bottom: solid 1px #cb0200;
	text-align: center;
	font-size: 14px;
	color: #fff;
}
.active-tool-top a:hover, .active-tool-bottom:hover {
	text-decoration: none;
	background: #b10401;
}
.active-tool-bottom {
	position: absolute;
	left: 0;
	bottom: -10px;
	width: 98px;
	height: 30px;
	border: solid 1px #cb0200;
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
	background: #ff0037;
	font-size: 14px;
	color: #fff;
	text-align: center;
	line-height: 30px;
}
