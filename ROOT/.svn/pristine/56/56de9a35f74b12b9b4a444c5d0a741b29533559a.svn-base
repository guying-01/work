
body{
	min-width: 300px;
    max-width: 768px;
    margin: 0 auto;
}
.live_fish_title:after,
.live_fish_titile_link:after,
.live_fish_wrap_b_fish:after,
.activity:after,
.conve-list:after,
.brand_zone:after,
.latest_product_wrap_b:after,
.miaosha_bottom:after,
.fresh_b_item:after,
.fresh_b:after,
.brand_wrap h4 div:after,
.icon12:after,.ff_koubei_hb:after {
	content: '.';
	clear: both;
	visibility: hidden;
	display: block;
	height: 0;
}

.live_fish_title {
	width: 100%;
	background: #eeeeee;
}

.live_fish_titile_link {
	float: left;
}

.live_fish_titile_link li,
.live_fish_wrap_t {
	float: left;
	height: 0.5rem;
	line-height: 0.5rem;
	font-size: 0.24rem;
	margin-left: 0.2rem;
}
.live_fish_titile_link .live_fish_wrap_t{
	margin-left: 0px;
	text-align: center;
	min-width: 0.82rem;
	white-space: nowrap;
	overflow: hidden;
	/*text-overflow: ellipsis;*/
	margin-left: 0.1rem;
}
.live_fish_wrap_t {
	margin-left: 4%;
}

@media only screen and (min-width: 321px) {
	.live_fish_titile_link li {
		margin-left: 0.3rem;
	}
}

.live_fish_titile_link li a {
	color: #000;
}

.live_fish_wrap_b_fish {
	width: 96%;
	margin-left: 3%;
	margin-top: 0.25rem;
	margin-bottom: 0.16rem;
}

.live_fish_wrap_b_fish li {
	float: left;
	width: 22%;
