.con9 .contact_us{
	color: #fff;
	background: #f55b41;
	border-radius: 0.2rem;
	margin-left: 0.2rem;
	display: block;
	width: 1rem;
	padding: 0.05rem 0.1rem;
	text-align: center;
}
.video_wrap{
	margin:0.7rem auto;
	width:6rem;
	height:3.8rem;
	padding: 0.16rem 0.12rem;
	box-sizing: border-box;
	background: url(../img/video_bg.jpg) no-repeat center center;
}
.video_wrap embed{
	width: 100%;
	height: 100%;
}
.baodao_box{
	margin-top:2.1rem;
	margin-left: 0.88rem;
	overflow: hidden;
}
.baodao_box:after,.name_box:after,.phone_qq_box:after,.need_box:after{
	content: '.';
	clear: both;
	visibility: hidden;
	height: 0;
	display: block;
}
.baodao_box{
	width: 4.78rem;
}
.baodao_item{
	width:2.09rem;
	margin-right:0.46rem;
	margin-bottom: 0.26rem;
	float: left;
}
.baodao_item1{
	width: 100%;
	margin: auto;
	padding-bottom: 0.26rem;
}
.baodao_item_pic{
	display: block;
	width:2.09rem;
	height: 2.11rem;
	border: 3px solid #ffd308;
}
.baodao_item1 .baodao_item_pic{
	margin: auto;
}
.baodao_item_pic img{
	height: 100%;
	width: 100%;
}
.baodao_item_t{
	text-align: center;
	width:100%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	font-size:0.18rem;
	color:#ffd308;
	font-family: "microsoft yahei";
	margin-top:0.12rem;
}

.busniess_info{
	text-align: center;
	color:#272636 ;
	font-size:0.22rem ;
	width: 5rem;
	background: #fff;
	padding: 0.25rem;
	margin: 0.2rem auto;
	border-radius: 0.2rem;
}
.contact_box{
	margin-top: 0.58rem;
	margin-left: 0.2rem;
	width: 5.97rem;
	height: 4.9rem;
	box-sizing: border-box;
	padding-top: 0.42rem;
	padding-left: 0.1rem;
}
.contact_box input{
	box-sizing: border-box;
	outline: none;
	width: 1.8rem;
	height: 0.34rem;
	line-height: 0.34rem;
	padding:0 6px;
	border: 1px solid #a6a6a6;
	font-size: 0.18rem;
}
.tips{
	font-size: 0.18rem;
	color:#272636;
	margin-right: 0.06rem;
}
.tips_qq{
	margin-left: 0.07rem;
}
.name_box,.phone_qq_box,.need_box{
	margin-bottom:0.22rem;
}
.need_box textarea{
	width: 4.75rem;
	height:2.34rem;
	outline: none;
	border:1px solid #a6a6a6;
	padding:0 10px;
	box-sizing: border-box;
}
.contact_box .tips,.contact_box input,.need_box textarea{
	float: left;
}
.send{
	width: 1.27rem;
	height:0.36rem;
	line-height:0.36rem;
	background: #f45a40;
	text-align: center;
	color:#fff;
	font-size:0.18rem;
	border-radius:0.14rem;
	margin:0.3rem auto 0;
	font-family: "microsoft yahei";
	box-shadow: 0px 1px 2px #000;
}
