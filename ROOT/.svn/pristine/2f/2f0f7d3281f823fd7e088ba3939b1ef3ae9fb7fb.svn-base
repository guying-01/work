.tit i {
	padding: 0 4px;
	margin-right: 8px;
	background: url(../../img/common/ico01.png) -213px -207px no-repeat;
}
/*确认订单页面新增h4样式 2016-11-2 by kuban*/
.ff_h4{
	font-size:0.28rem;
	color: #666;
	box-sizing: border-box;
	padding:0.1rem;
}
.totalOrderId{
	font-size: 0.28rem;
}
.previlageShow1{
	font-size: 0.24rem!important;
	font-weight: normal;
	line-height: 0.4rem;
}
/*确认订单页面新增h4样式结束*/
.table-line tr th{
	font-size: 0.22rem;
}
.table-line tr td{
	font-size: 0.2rem;
	padding:0.1rem;
	line-height: 0.3rem;
}
#sumTotal{
	font-size: 0.3rem;
}
.big-button{
	margin:0;
	float: right!important;
}
.total_price_box:after{
	content: '.';
	clear: both;
	visibility: hidden;
	height: 0;
	display: block;
}
.total_price_box{
	width: 100%;
	position:fixed;
	left: 0;
	bottom:0;
	box-sizing: border-box;
	padding-left: 0.1rem;
    border-top: 1px solid #f8f8f8;
    background: #fff;
}
.total_price_box span{
	font-size: 0.24rem;

}
.total_price{
	height: 0.7rem;
	line-height: 0.7rem;
	float: right;
	margin-right: 0.1rem;
}
.tips{
	padding:0.1rem;
}
.order-note{
	box-sizing: border-box;
	padding:0.16rem;
}
.order-note p{
	padding:0;
	margin-bottom: 0.14rem;
}
.order-note p,.order-note strong,.order-note a{
	font-size: 0.26rem;
}
.order{
	padding-bottom: 0.2rem;
}
.table_bgA{
	background: #fff;
}
.my_clear:after{
	content: '';
	display: block;
	clear: both;
}
.choosePayment{
	/*display: none;*/
}
.choosePayment .titH{
	padding-top: 0.4rem;
}
#pay_balance,#pay_weixin{
	border-top: 1px solid #ececec;
	padding-left: 0.3rem;
	line-height: 1rem;
	display: block;
	height: 1rem;
}
#pay_weixin{
	border-bottom: 1px solid #ececec;
}
#pay_balance span,#pay_weixin span{display: block;width: 0.6rem;height: 1rem;overflow: hidden;float: left;padding-right: 0.3rem;}
#pay_balance span img,#pay_weixin span img{width: 0.66rem;vertical-align: middle;}
#pay_balance p,#pay_weixin p{float: left;font-size: 0.26rem;padding-right: 3.2rem;}
#pay_balance i,#pay_weixin i{display: block;float: left;height: 0.4rem;width: 0.4rem;border-radius: 1.5rem;border: 1px solid #dcdcdc;margin-top: 0.28rem;}
#choosePayment .isChecked {
	background: url("../../img/cart/checked.png") no-repeat center center;
	background-size: cover;
}