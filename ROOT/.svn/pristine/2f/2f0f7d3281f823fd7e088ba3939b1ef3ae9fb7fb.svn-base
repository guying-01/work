.wulua-content-right ul {
}
.wulua-content-right ul li {
	line-height: 0.4rem;
	font-size: 0.24rem;
	padding-top: 0.1rem;
	padding-left: 0.35rem;
}
.wulua-content-right ul li p {
	color: #db0401;
}
.wulua-content-right ul li span {
}
.wulua-content-right-foc {
	background-color: #faf8f7;
}
.wulua-three {
	clear: both;
	width: 100%;
	border-top: 1px solid #e4d0ad;
	background: #ffffe5;
	line-height: 0.4rem;
	font-size: 0.24rem;
	margin-top: 0.1rem;
	margin-bottom: 0.1rem;
	text-align: center;
}
.wulua-three i {
	padding: 0 15px;
	background: url(../../img/common/ico01.png) no-repeat -255px -266px;
}
