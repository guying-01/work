<!--此处是头部购物车模板开始 id="header_shopcar"-->
<script type="text/x-handlebars-template" id="header_shopcar">
    {{#each this}}
    <li>
        <div class="leftD"><img style="width:100%; height:100%;" src="{{itemPic}}"/></div>
        <div class="centerD">{{goodsName}}</div>
        <div class="rightD">
            <p>￥<span>{{salesPrice}}</span>x<span>{{amount}}</span></p>
            <a href="##" class="delete_a" itemId={{id}}>删除</a>
        </div>
    </li>
    {{/each}}
</script>
<!--此处是头部购物车模板结束-->

