/*饭饭口碑库和订阅商机弹出层开始*/
.order_business_hb_box{
	display: none;
	position: fixed;
	z-index:1000;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.6);
}
.order_business_hb{
	position: absolute;
	box-sizing: border-box;
	padding: 0.3rem 0;
	width:5.19rem;
	height:4.2rem ;
	background: #fff;
	box-sizing: border-box;
}
.order_business_hb li{
	float: left;
	width: 50%;
	height:1.8rem;
	text-align: center;
	font-size:0.22rem ;
}
.order_business_hb li a{
	color:#000;
}
.order_bus_icon1{
	display: block;
	margin:0.43rem auto 0.19rem;
	width:0.71rem ;
	height:0.54rem ;
	background: url(/resources/basepage/store/default/level_pages/img/fflv2_img/order_bus_icon1.png) no-repeat center center;
	background-size: 100% 100%;
}
