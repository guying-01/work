/*加减*/
.num_box {
	position: relative;
	display: block;
	padding: 0 0.3rem;
	height: 0.3rem;
	overflow: hidden;
	/*border: solid 1px #ccc;*/
}
.num_box input {
	position: relative;
	z-index: 0;
	display: block;
	margin: 0;
	width: 0.59rem;
	height: 0.28rem;
	border: 0;
	font-family: Arial, Helvetica, sans-serif;
	text-align: center;
	line-height: 24px;
	line-height: 0.28rem;
	outline: none;
}
.num_box a {
	position: absolute;
	display: block;
	top: 0;
	width: 0.38rem;
	text-indent: -99999em;
}
.reduce_1 {
	left: 0;
	background: url(../../img/common/count.png) no-repeat -100px center;
}
.add_1 {
	right: 0;
	background: url(../../img/common/count.png) no-repeat -50px center;
}
