/*左侧菜单样式*/
.menu {
	position: relative;
	z-index: 11;
	width: 1200px;
	height: 0;
	margin: 0 auto;
}
.menu a:hover{ text-decoration:none}
.menu-div {
	position: absolute;
	z-index: 12;
	display: none;
	top: 0;
	left: 0;
	width: 150px;
	min-height: 370px;
	background: rgba(0,0,0,0.6);
	background: #333\9;
}
.menu h2 {
	position: absolute;
	top: -50px;
	left: 0;
	width: 150px;
	height: 50px;
	background: url(../../img/common/tit_bg_1.png) no-repeat;
	background-size: 100% 100%;
	color: #fff;
	font-size: 14px;
	text-align: center;
	line-height: 50px;
}


.menuB {
	cursor: pointer;
}
.menuB .menu-div {
	display: block;
}
.menuB h2 i {
	background: url(../../img/common/ico01.png) -213px -49px !important;
}
.submenu {
	width: 150px;
	height: 36px;
	line-height: 36px;
	margin: 5px auto;
}
.submenu h3 {
	display: block;
	width: 150px;
	color: #fff;
	text-align: center;
	font-weight: lighter;
	cursor: pointer;
}
.submenu h3 span {
	display: none;
	float: right;
	margin-right: 30px;
	font-size: 13px;
}
.submenu h3:hover span {
	display: inherit;
	text-decoration: none !important;
}

.submenu:hover h3 {
	background: url(../../img/common/tit_bg_2.png) no-repeat;
	background-size: 100% 100%;
}
.submenu-floating {
	position: absolute;
	display: none;
	padding: 10px;
	width: 346px;
	min-height: 349px;
	border-bottom: solid 1px #dc1913;
	border-right: solid 1px #dc1913;
	background: rgba(255,255,255,0.9);
	background: #fff\9;
}
.submenu:hover .submenu-floating ul {
	display: block;
}
.submenu .submenu-floating {
	top: 0;
	left: 150px;

}
.submenu-floating ul {
	width: 350px;
	padding-bottom: 15px;
	height: 70px;
	overflow: hidden;
}
.submenu-floating ul h4 {
	margin: 0 10px 7px 10px;
	height: 30px;
	border-bottom: solid 1px #ddd;
	font-size: 14px;
	color: #000;
	font-weight: lighter;
	line-height: 30px;
}
.submenu-floating ul h4 a {
	float: right;
	font-family: Arial, Helvetica, sans-serif;
}
.submenu-floating ul li {
	display: inline-block;
	padding: 0 10px;
	float: left;
	line-height: 22px;
	white-space: nowrap;
}
.submenu-floating ul li a {
	color: #777;
}
.submenu-floating ul li a:hover {
	color: #f59909;
}

.more-submenu {
	width: 150px;
	height: 36px;
	border-bottom: solid 1px #c60401;
	background: #db0401;
	line-height: 36px;
	text-align: center;
}
.more-submenu span {
	color: #fff;
}
.more-submenu i {
	margin-left: 5px;
	padding: 0 8px;
	background: url(../../img/common/ico01.png) -213px -49px;
}
.more-submenuI {
	background: url(../../img/common/ico01.png) -213px -103px !important;
}
