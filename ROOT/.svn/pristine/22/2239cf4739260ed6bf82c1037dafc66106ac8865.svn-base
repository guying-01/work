@charset "utf-8";
/* CSS Document */
body {
	background: #f5f5f5;
}
.banner {
	position: relative;
	z-index: 0;
	width: 100%;
	height: 370px;
	overflow: hidden;
}
.newPutGoods {
	height: 0.7rem;
}
.min-banner {
	width: 100%;
	height: 370px;
}
.min-banner li {
	display: none;
	width: 100%;
	height: 370px;
	text-align: center;
}
.min-banner li  a{ display: block; width:100%; height:100%;}
.sliderBtn {
	background: url(../../img/index/sprite.png) no-repeat;
	cursor: pointer;
	height: 60px;
	position: absolute;
	top: 150px;
	left: 50%;
	z-index: 9;
	width: 0;
}
.prev {
	margin-left: -385px;
}
.next {
	background-position: right top;
	margin-left: 385px;
}
.banner_num {
	position: absolute;
	left: auto !important;
	right: 230px !important;
	bottom: 10px;
	width: auto !important;
	height: 12px;
	padding: 5px 7px;
	background: url(../../img/common/pngB.png) repeat;
	text-align: center;
	border-radius: 10px;
}
.banner_num li {
	float: left;
	width: 12px;
	height: 12px;
	margin: 0 3px;
	cursor: pointer;
	overflow: hidden;
	background: url(../../img/index/roundness.png) 0 0 no-repeat;
}
.banner_num li:hover, .btnListOn {
	background: url(../../img/index/roundness.png) 0 -13px no-repeat !important;
}
.bannerUpAd {
	width: 1200px;
	position: relative;
	height: 0;
	margin: 0 auto;
}
.bannerUpAdImg {
	position: absolute;
	top: -370px;
	right: 0;
	width: 165px;
	height: 370px;
}
.bannerUpAdImg a, .bannerUpAdImg img {
	display: block;
	width: 165px;
	height: 185px;
}
/*推荐产品*/
.recommend {
	margin-top: 20px;
/*	height: 630px;*/
}
.recommend .tabT {
	height: 25px;
	margin-bottom: 10px;
	background: #faf2f2;
	border: solid 1px #fcd8d8;
}
.recommend .tabT li {
	position: relative;
	width: 200px;
	float: left;
	line-height: 25px;
	text-align: center;
	font-size: 14px;
	cursor: pointer;
	border-right: solid 1px #fcd8d8;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.recommend .tabT .on {
	background: #f69191;
	color: #fff;
}
.recommend .tabT .on i {
	display: block;
	position: absolute;
	bottom: -8px;
	left: 92px;
	width: 0;
	height: 0;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-top: 8px solid #f69191;
}
.recommend-produce-div {
	width: 1200px;
	height: 590px;
	overflow: hidden;
	background: #fff;
}
.recommend-produce {
	position: relative;
	float: left;
	width: 198px;
	height: 293px;
	border: solid 1px #efefef;
}
.recommend-produce:hover {
	border: solid 1px #f69191;
}
.recommend-produce img {
	display: block;
	margin: 25px auto 10px auto;
	width: 140px;
	height: 140px;
}
.recommend-price {
	position: relative;
	width: 170px;
	height: 80px;
	margin: 0 auto;
	padding: 5px 0 0 0;
	line-height: 22px;
}
.recommend-price p, .recommend-price p b {
	color: #c81623;
}
.recommend-price p strong {
	color: #c81623;
}
.recommend-price p u {
	color: #888;
}
.recommend-price h3 {
	height: 40px;
	margin-bottom: 5px;
	overflow: hidden;
	font-weight: lighter;
	line-height: 20px;
}
.recommend-price p {
	clear: both;
	height: 25px;
	line-height: 25px;
}
.recommend-price p strong {
	font-size: 16px;
}
.recommend-price p u {
	font-size: 13px;
	text-decoration: line-through;
}
/*楼层产品*/
.floor {
	margin-top: 30px;
	height: 510px;
	background: #fff;
}
.floor h2 {
	position: relative;
	height: 49px;
	line-height: 24px;
}
.floor h2 span {
	display: block;
	float: left;
	font-size: 16px;
	color: #fff;
	font-weight: lighter;
	text-align: center;
	line-height: 49px;
	height: 49px;
	width: 190px;
}
.floor h2 .tabT {
	position: absolute;
	left: 200px;
	top: 0;
}
.floor h2 .tabT li {
	position: relative;
	display: block;
	float: left;
	margin: 0 20px;
	font-weight: lighter;
	height: 49px;
	line-height: 49px;
	cursor: pointer;
}
.floor h2 .tabT li.on {
	font-size: 14px;
	font-weight: bold;
}
.floor h2 .tabT li.on i {
	position: absolute;
	bottom: 0;
	left: 50%;
	width: 14px;
	height: 6px;
	margin-left: -7px;
}
.floor-produce {
	height: 460px;

}
/*
.floor-produceA, .floor-produceA img {
	float: left;
	width: 190px;
	height: 460px;
}
*/

.floor-produceA{
	float: left;
	width: 190px;
	height: 460px;
}
.floor-produceA img {
	margin-left:-28px;
	width: 218px;
	height: 460px;

}

.floor-produceB, .floor-produceB img {
	float: left;
	width: 404px;
	height: 460px;
}
.floor-produceC {
	float: right;
	/*width: 606px;*/
	width: 1010px;
}
.floor-produceCimg li img{ width:160px !important; height:188px !important;margin:20px !important;}
.floor-produceC li {
	float: left;
	width: 200px;
	height: 228px;
	border: solid 1px #fff3f3;
}
.floor-produceC li:hover {
	border: solid 1px #db0401;
}
.floor-produceC li img {
	display: block;
	margin: 10px auto 5px auto;
	width: 120px;
	height: 120px;
}
.floor-produceC li h3 {
	width: 150px;
	height: 38px;
	margin: 0 auto;

	overflow: hidden;
	line-height: 19px;
	font-weight: lighter;
}
.floor-produceC li h3 a:hover {
	color: #db0401;
}
.floor-produceC li p {
	width: 150px;
	height: 20px;
	margin: 0 auto;
	padding: 5px 0 0 0;
	overflow: hidden;
	line-height: 20px;
}
.floor-produceC li p b {
	font-size: 12px;
	color: #ca1d3f;
	font-family: "微软雅黑", "Microsoft Yahei", Arial
}
.floor-produceC li p strong {
	padding-right: 10px;
	font-size: 16px;
	color: #ca1d3f;
}
.floor-produceC li p u {
	color: #888;
	text-decoration: line-through;
}
