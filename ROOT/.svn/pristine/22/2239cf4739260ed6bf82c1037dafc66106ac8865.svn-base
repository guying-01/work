/*range6*/
.hot {
	position: absolute;
	top: 10px;
	left: 0px;
}
.hot p {
	position: relative;
	float: left;
	width: 43px;
	height: 43px;
	margin-left: 10px;
}
.hotA {
	background: url(../../img/index/r1.png) no-repeat;
	_background: url(../../img/index/r1.gif) no-repeat;
}
.hotB {
	background: url(../../img/index/r2.png) no-repeat;
	_background: url(../../img/index/r2.gif) no-repeat;
}
.hotC {
	background: url(../../img/index/r3.png) no-repeat;
	_background: url(../../img/index/r3.gif) no-repeat;
}
.hot i, .hot u {
	position: absolute;
	display: block;
}
.hot i {
	top: 6px;
	left: 8px;
	color: #fff;
	font-size: 12px;
}
.hot u {
	top: 18px;
	left: 23px;
	font-size: 12px;
}
.hotB i {
	top: 5px;
	left: 10px;
	font-size: 14px
}
.hotA u {
	color: #e60012;
}
.hotB u {
	color: #22ac38;
}
.hotC u {
	color: #f39800;
}

/*品牌推荐*/
.brand{height:300px;}
.brand .recommend-produce{height:148px}
.brand .recommend-produce img{ width:150px; height:110px; margin:20px auto;}
.brandDiv{position: relative;height:29px; }
.brandText{position: absolute;
top: 6px;
left: 0;}
.recommend .brandDiv .tabT {
height: 29px;
margin-bottom: 0;
background: none;
border:none;
}
.recommend .brandDiv .tabT  li{float:right;width:150px;line-height:27px;border:none;}

.recommend .brandDiv .tabT .on {
	background:none;
border-bottom:solid #222 2px;
color:#222;
}
