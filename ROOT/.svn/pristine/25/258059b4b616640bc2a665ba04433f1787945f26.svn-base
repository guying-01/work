}
.member-tab-bank {
	height: 2.15rem;
	margin-bottom: 0.87rem;
}
.member-tab-bank li span{
	font-size: 0.14rem;
	line-height: 0.44rem;
}
/*.member-tab-bank h2 {*/
	/*height: 0.48rem;*/
/*}*/
.member-tab-bank .tabT li .changeBank {
	background: url("../../img/member/changeBank.png") no-repeat center center;
	background-size: 100%;
	width: 0.47rem;
	/*margin-left: 0.2rem;*/
}
.member-tab-bank .tabT li .getBank {
	background: url("../../img/member/getBank.png") no-repeat center center;
	background-size: 100%;
	width: 0.54rem;
	/*margin-left: 0.05rem;*/
}
.member-tab-bank .tabT li .rechange {
	background: url("../../img/member/recharge.png") no-repeat center center;
	background-size: 100%;
	width: 0.54rem;
}
.member-tab-bank .bank-all {
	overflow: hidden;
}
.member-tab-bank .bank-all .tabT-left {
	float: left;
	width: 59%;
	border-right: 1px solid #ddd;
}
.member-tab-bank .bank-all .tabT-left li {
	position: static;
	/*margin-left: 0.3rem;*/
}
.member-tab-bank .bank-all .tabT-right {
	width: 40.5%;
	float: right;
}
.member-tab-bank .bank-all .tabT-right li{
	float: none;
	padding-top: 0.05rem;
	border-bottom: 1px solid #ddd;
	margin-left: 0;
	padding-left: 0.2rem;
}
.member-tab-bank .bank-all .tabT-right li span {
	line-height: 0.58rem;
	vertical-align: middle;
	padding-left: 0.2rem;
}
.member-tab-bank .bank-all .tabT-right .settingPwd {
	display: inline-block;
	height: 0.36rem;
	width: 0.32rem;
	background: url("../../img/member/settingPwd.png") no-repeat;
	background-size: 100% 100%;
	vertical-align: middle;
}
.member-tab-bank .bank-all .tabT-right .settingUser {
	display: inline-block;
	height: 0.36rem;
	width: 0.32rem;
	background: url("../../img/member/settingUser.png") no-repeat;
	background-size: 100% 100%;
	vertical-align: middle;
}
.serverManage {
	width: 100%;
}
.serverManage li {
	width: 49%;
	height: 0.87rem;
}
.serverManage li:nth-child(1) {
	border-right: 1px solid #dddddd;
}
.serverManage li a {
	margin: 0 auto;
	width: 67%;
}
.serverManage .tabT li .scoreM {
	float: left;
	background: url("../../img/member/scoreM.png") no-repeat;
	float: left;
	background-size: 100% 100%;
	width: 0.72rem;
}
.serverManage .tabT li .backGm {
	float: left;
	background: url("../../img/member/backG.png") no-repeat;
	background-size: 100% 100%;
	float: left;
	width: 0.8rem;
}
.serverManage .tabT li .scoreMs,.backGs {
	line-height: 0.7rem!important;
	margin-left: 0.1rem;
}
.member-order-list {
	width: 996px;
	margin-left: -1px;
}
.member-order {
	position: relative;
	float: left;
	width: 201px;
	height: 160px;
	margin-top: 15px;
	padding: 20px 0 0 130px;
	border-left: solid 1px #f5f5f5;
}
.member-order-info {
	clear: both;
	width: 200px;
	min-height: 25px;
	line-height: 25px;
}
.member-order-info span, .member-order-info p {
	display: block;
	float: left;
	color: #666;
}
.member-order-info p {
	width: 130px;
	line-height: 18px;
	padding-top: 3px;
}
.member-order-info span {
	width: 65px;
	text-align: right;
}
.pay-btn {
	position:absolute; bottom:18px; right:15px;
	display: block;
	width: 70px;
	height: 25px;
	background: #f88c0e;
	text-align: center;
	color: #fff;
	line-height: 25px;
}
.pay-btn:hover{ background:#ea800d; text-decoration:none;}
.order-price {
	display: block;
	position:absolute; bottom:20px; right:100px;
	font-size: 14px;
	font-family: Arial, Helvetica, sans-serif;
}
.order-price b {
	font-family: "微软雅黑", Arial;
}
.order-img {
	position: absolute;
	top: 15px;
	left: 20px;
	width: 100px;
	height: 100px;
	border: solid 1px #ebebeb;
}
.order-img img {
	width: 100px;
	height: 100px;
}
.order-img u {
	position: absolute;
	top: -7px;
	left: 2px;
	display: block;
	width: 96px;
	height: 6px;
	background: url(../../img/member/imgbg.png) no-repeat
}
.order-detail {
	position: absolute;
	bottom: 20px;
	left: 20px;
	color: #f88c0e;
}
.member-collection {
	width: 665px;
	height: 310px;
	margin-top: 10px;
	background: #fff;
}
.member-goods {
	position: relative;
	width: 540px;
	height: 240px;
	margin: 15px auto 0 auto;
}
.member-goods .pre, .member-goods .next {
	position: absolute;
	top: 85px;
	width: 30px;
	height: 45px;
	cursor: pointer;
}
.member-goods .pre {
	background: url(../../img/member/pre.png) no-repeat;
	left: -45px;
}
.member-goods .next {
	background: url(../../img/member/next.png) no-repeat;
	right: -45px;
}
.member-goods-ul {
	width: 540px;
	height: 240px;
	overflow: hidden;
}
.member-goods ul {
	width: 100000px;
}
.member-goods ul li {
	float: left;
	width: 178px;
	height: 238px;
	border: solid 1px #fff;
}
.member-goods ul li:hover {
	border: solid 1px #db0401;
}
.member-goods ul li img {
	display: block;
	width: 150px;
	height: 150px;
	margin: 14px auto 0 auto;
}
.member-goods ul li h3 {
	width: 150px;
	height: 36px;
	margin: 0 auto;
	padding-top: 4px;
	overflow: hidden;
	font-weight: lighter;
	line-height: 18px;
}
.member-goods ul li h3 a:hover {
	color: #db0401;
}
.member-goods ul li p {
	width: 150px;
	height: 16px;
	margin: 5px auto 0 auto;
	padding-top: 4px;
	overflow: hidden;
	line-height: 16px;
}
.member-goods ul li p strong {
	padding-right: 10px;
	color: #ca1d3f;
	font-family: Arial, Helvetica, sans-serif;
}
.member-goods ul li p strong b {
	font-family: "微软雅黑", Arial;
	color: #ca1d3f;
}
.member-goods ul li p u {
	color: #a6a6a6;
	text-decoration: line-through;
}
.member-footprint {
	display: block;
	float: right;
	width: 320px;
	height: 630px;
	margin-top: 10px;
	background: #fff;
}
.member-footprint-con {
	width: 230px;
	height: 550px;
	margin: 20px auto 0 auto;
	border-left: solid 1px #d9d9d9;
}
.member-footprint-con ul {
	padding-bottom: 10px;
}
.member-footprint-con ul h3 {
	position: relative;
	height: 15px;
}
.member-footprint-con ul h3 u {
	position: absolute;
	display: block;
	top: 0px;
	left: -6px;
	width: 11px;
	height: 11px;
	background: url(../../img/common/ico01.png) -266px -53px;
}
.member-footprint-con ul h3 p {
	position: absolute;
	display: block;
	top: -5px;
	left: 15px;
	font-weight: lighter;
}
.member-footprint-con ul li {
	float: left;
	width: 80px;
	margin: 0 0 20px 25px;
	height: 100px;
	color: #ca1d3f;
	font-family: Arial, Helvetica, sans-serif;
	font-weight: bold;
	text-align: center;
	line-height: 20px;
}
.member-footprint-con ul li b {
	color: #ca1d3f;
	font-family: "微软雅黑", Arial;
}
.member-footprint-con ul li img {
	width: 80px;
	height: 80px;
}
.main {
	width: 100%;
}
.member-list {
	margin: 0 auto;
	width: 100%;
}
.member-list .tabT {

	border-bottom: solid 1px #f2f2f2;
	margin-top: 0.03rem;
	overflow: hidden;
}
.member-list .tabT li {
	float: left;
	width: 3rem;
	height: 0.6rem;
}
.member-list .tabT li span {
	width: 3rem;
	display: block;
	line-height: 0.6rem;
	border-right: solid 1px #f2f2f2;
	border-top: solid 1px #f2f2f2;
