.content-right {

}

.content-right .goBack {
	display: inline-block;
	height: 0.5rem;
	width: 0.35rem;
	background: url(../../img/member/goBack.png) no-repeat center center;
	background-size: contain;
	position: absolute;
	left: 0.2rem;
	top:50%;
	margin-top: -0.25rem;
	text-decoration: none;
}
/*首页样式*/
.user {
	position: relative;
	z-index: 1;
	height: 2rem;
	padding-left: 1.6rem;
}
.user_total {
	background: url("../../img/member/userBg.png") no-repeat;
	background-size: cover;
	height: 2rem;
	border-bottom: 0.1rem solid #dddddd;
}
.user .user_desc {
	padding-left: .42rem;
	position: relative;
	height: 100%;
}
.user .user_desc .safe_setting {
	font-size: 0.23rem;
	color: white;
	line-height: 0.5rem;
}

.member-header {
	position: absolute;
	top: 0.2rem;
	left: .4rem;
	width: 1.2rem;
	height: 1.2rem;
	border-radius: 50%;
	-moz-border-radius: 50%;
	-webkit-border-radius: 50%;
	-o-border-radius: 50%;
	text-align: center;
}
.member-header .pic_user {
	width: 1.2rem;
	height: 1.2rem;
	border-radius: 0.6rem;
	overflow: hidden;
}

.member-header img {
	width: 1.2rem;
	height: 1.2rem;
}
.member-header a {
	position: absolute;
	z-index: 2;
	display: none;
	top: 0px;
	left: 0px;
	width:1.2rem;
	height: 1.2rem;
	background: url(../../img/common/png.png) repeat;
	color: #fff;
	font-size: 14px;
	text-align: center;
	line-height: 112px;
}
.member-header:hover a {
	display: block;
}
.member-header span {
	font-size: .21rem;
	color: white;
	line-height: .5rem;
}
.user-name {
	font-size: 0.27rem;
	font-family: Arial, Helvetica, sans-serif;
	padding-top: .4rem;
	line-height: .48rem;
	color: white;
}
.user-information {
	height: 35px;
}

.user-load-pop {
	display: block;
	float: left;
	cursor: pointer;
}
.user-information img {
	margin: 7px 5px 0 0;
}
.user-load {
	position: absolute;
	display: block;
	z-index: 7;
	top: 95px;
	left: 131px;
	width: 0px;
	height: 0px;
	overflow: hidden;
	padding: 10px 15px 0 15px;
	background: url(../../img/member/lv-bg.png) no-repeat;
}
.user-load h3 {
	padding-top: 10px;
	font-weight: lighter
}
.user-load-con {
	position: relative;
	width: 200px;
	height: 45px;
	margin: 5px auto 0 auto;
}
.user-load-con .loading {
	width: 200px;
	margin: 20px auto 0 auto;
}
.user-load-con span {
	display: block;
	position: absolute;
	font-family: Arial, Helvetica, sans-serif;
}
.load-Tl {
	top: 3px;
	left: 0px;
	color: #a5a5a5;
}
.load-Tr {
	top: 3px;
	right: 0px;
	color: #a5a5a5;
}
.load-Bl {
	top: 30px;
	left: 0px;
}
.load-Br {
	top: 30px;
	right: 0px;
}
.load-ofc {
	position: absolute;
	z-index: 9;
	top: -3px;
	padding: 0 5px;
	background: #f88c0e;
	color: #fff;
	border-radius: 2px;
	font-family: Arial, Helvetica, sans-serif;
}
.load-ofc i {
	position: absolute;
	bottom: -8px;
	left: 50%;
	margin-left: -4px;
	width: 0;
	height: 0;
	overflow: hidden;
	border-width: 4px;
	border-style: solid dashed dashed;
	border-color: #f88c0e transparent transparent;
}
.user-load-pop:hover .user-load {
	width: 202px;
	height: 89px;
}
.user-information p {
	line-height: 0.48rem;
	font-size: 0.23rem;
	color: white;
}
.user-information b {
	display: block;
	float: left;
	margin-left: 3px;
	font-size: 0.23rem;
	line-height: 0.48rem;
	color: white;
}
.user-information a {
	display: block;
	float: left;
	line-height: 35px;
	font-size: 14px;
	color: #db0401;
}
.user-information-ico {
	float: left;
	width: 50px;
	height: 21px;
	padding-left: 24px;
	margin: 5px 0 0 10px;
	background: url(../../img/member/Award_points.png) no-repeat;
	line-height: 21px;
	color: #fff;
}
.loading {
	float: left;
	width: 1.1rem;
	margin: 0.18rem 0.2rem 0 0.1rem;
	overflow: hidden;
	background: #fff;
	border-radius: 0.2rem;
}
.loading p {
	margin: 0;
	width: 0;
	height: 0.18rem;
	background: #f88c0e;
	color: #fff;
	border-radius: 0.2rem;
	border: 0.03rem solid #ededed;
	box-sizing: border-box;
}
.assets {
	float: right;
	width: 320px;
	height: 170px;
	margin-bottom: 10px;
	background: #fff;
}
.member-tit {
	padding-left: 0.22rem;
	border-bottom: solid 1px #dcdcdc;
	font-size: 0.24rem;
	line-height: 0.48rem;
}
.member-tit i {
	padding: 0 4px;
	margin-right: 8px;
	background: url(../../img/common/ico01.png) -213px -207px no-repeat;
}
.member-tit a {
	float: right;
	color: #656565;
	font-weight: lighter;
	font-size: 12px;
}
.assets-con {
	height: 50px;
	padding-left: 50px;
	overflow: hidden;
}
.assets-con img {
	display: block;
	float: left;
	margin: 20px 30px 0 0;
}
.assets-con b {
	display: block;
	float: left;
	max-width: 150px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size: 48px;
	font-weight: lighter;
	font-family: Arial, Helvetica, sans-serif;
	color: #f24a4a;
}
.assets-con span {
	display: block;
	float: left;
	margin: 25px 0 0 10px;
	font-size: 14px;
}
.member-tab {
	position: relative;
	clear: both;
	width: 100%;
	background: #fff;
}
.member-tab .tabT {
	width: 100%;
	border-bottom: 0.1rem solid #dddddd;
}
.member-tab .tabT li {
	float: left;
	font-size: 0.22rem;
	color: #707271;
	/*margin-left: 0.65rem;*/
	/*padding-top: 0.2rem;*/
	position: relative;
}
#message-gy{
	position: absolute;
	top:-260%;
	right: 0.1rem;
	z-index: 99;
}
#message-gy span{
	color: #fff;
	font-size: 0.23rem;
}
.member-tab .allOrderList li:nth-of-type(2) i{
	background: url("../../img/member/allOrderList.png") no-repeat center center;
	background-size: contain;
}
.member-tab .allOrderList li:nth-of-type(3) i{
	background: url("../../img/member/all-house.png") no-repeat center center;
	background-size: contain;
}
.member-tab .allOrderList li:nth-of-type(4) i{
	background: url("../../img/member/all-collect.png") no-repeat center center;
	background-size: contain;
}
.member-tab .allOrderList li:nth-of-type(5) i{
	background: url("../../img/member/all-history.png") no-repeat center center;
	background-size: contain;
}
.member-tab .tabT li i {
	display: block;
	width: 0.56rem;
	height: 0.6rem;
	background: url("../../img/member/noPay.png") no-repeat center center;
	background-size: 100%;
	/*margin-left: 0.12rem;*/
}
.member-tab .tabT li .noAccept {
	background: url("../../img/member/noAccept.png") no-repeat center center;
	width: 0.52rem;
	height: 0.6rem;
	background-size: 100%;
/*	margin-left: 0.16rem;*/
}
.member-tab .tabT li .noSend {
	background: url("../../img/member/noSend.png") no-repeat center center;
	width: 0.76rem;
	height: 0.58rem;
	background-size: 100%;
}
.member-tab .tabT li .complete {
	background: url("../../img/member/complete.png") no-repeat center center;
	width: 0.55rem;
	height: 0.59rem;
	background-size: 100%;
}
.member-tab .tabT li .backGoods {
	background: url("../../img/member/backGoods .png") no-repeat center center;
	margin-left: 0.2rem;
	width: 0.55rem;
	height: 0.59rem;
	background-size: 100%;
}
.member-tab .tabT li span {
	font-size: 0.22rem;
	line-height: 0.44rem;
	/*padding-left: 0.05rem;*/
}

.member-tab .tabT li u {
	font-family: Arial, Helvetica, sans-serif;
	color: #f24a4a;
