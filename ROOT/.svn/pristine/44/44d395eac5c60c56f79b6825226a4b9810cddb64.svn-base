<script type="text/javascript">
    var counts = 0;
    var spot = 4;
    var namespot = 0;
    //CharMode函数
    //测试某个字符是属于哪一类.
    function CharMode(iN) {
        if (iN >= 48 && iN <= 57) //数字
            return 1;
        if (iN >= 65 && iN <= 90) //大写字母
            return 2;
        if (iN >= 97 && iN <= 122) //小写
            return 4;
        else
            return 8; //特殊字符
    }
    //bitTotal函数
    //计算出当前密码当中一共有多少种模式
    function bitTotal(num) {
        modes = 0;
        for (i = 0; i < 4; i++) {
            if (num & 1) modes++;
            num >>>= 1;
        }
        return modes;
    }
    //checkStrong函数
    //返回密码的强度级别
    function checkStrong(sPW) {
        if (sPW.length <= 4)
            return 0; //密码太短
        Modes = 0;
        for (i = 0; i < sPW.length; i++) {
            //测试每一个字符的类别并统计一共有多少种模式.
            Modes |= CharMode(sPW.charCodeAt(i));
        }
        return bitTotal(Modes);
    }
    //pwStrength函数
    //当用户放开键盘或密码输入框失去焦点时,根据不同的级别显示不同的颜色
    function pwStrengthFun(pwd) {
        var pwStrength = $("#pwStrength");
        if (pwd == null || pwd == '') {
            $("#pwdj").hide();
        }
        else {
            S_level = checkStrong(pwd);
            switch (S_level) {
                case 0:
                    break;
                case 1:
                    $("#pwdh").hide();
                    $("#pwdc").hide();
                    $("#pwds").show();
                    $("#pwdj").show();
                    pwStrength.val("1");
                    break;
                case 2:
                    $("#pwdh").hide();
                    $("#pwdc").hide();
                    $("#pwdj").hide();
                    $("#pwdm").show();
                    $("#pwds").hide();
                    pwStrength.val("2");
                    break;
                default:
                    $("#pwdc").hide();
                    $("#pwdj").hide();
                    $("#pwdh").show();
                    $("#pwdm").hide();
                    pwStrength.val("3");
            }
        }

        return;
    }
    $(function () {
        function biaoba() {
            var me = this;
            var qj = $("#qunaju");
            var mobile = $("#mobile");
            var ragex = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1})|(17[0-9]{1})|(14[0-9]{1}))+\d{8})$/;
            if (!ragex.test(mobile.val())) {
                $("#ms3").hide();
                $("#m1").show();//显示手机号码格式不正确 by 葛硕
                $("#m2").hide();

                $("#mobile").addClass("redborder");
                qj.val("youcuo");
                return false;
            }
            else if (!bushiba) {
                $("#ms3").hide();
                $("#m1").hide();//隐藏手机号码格式不正确 by 葛硕

                $("#m2").show();

                $("#mobile").addClass("redborder");
                qj.val("youcuo");
                return false;
            }
            else {
                var self = $("#code");
                callService("${ctx}/generateAuthCode.ajax", {mobile: mobile.val()}, {
                    type: 'GET',
                    self: self,
                    callSuccess: function (data) {
                        if (data.result == "false") {
                            $("#a4").show();
                            $(me).addClass("notShow");
                            turned();
                            return;
                        }
                        self.hide();
                        $('#message_info').show();
                        if (data.show)
                            jsAlert(data.msgContent);
                        if (data.result != "") {
                            $("#a4").hide();
                            $(me).addClass("notShow");
                            turned();
                        } else {
                            jsErrAlert("发送失败，请重试");
                        }

                    }
                })

            }
        }

        $("#code").click(biaoba);

        //倒计时
        function turned() {
            var interval;
            var self = $("#code").unbind('click');
            $(".cx2").show();
            $("#second").html('60');
            interval = setInterval(function () {
                var second = $.trim($("#second").html());
                if (second == 0) {
                    self.click(biaoba);
                    $("#message_info").hide();
                    $("#a4").hide();
                    self.show();
                    clearInterval(interval);
                }
                $("#second").html(--second);
            }, 1000);
        }

        <!--光标点进去时的提示-->
        $("input").focus(function () {
            $(this).siblings("p.loginError").show();
            /* ADD START BY geshuo 20151205: 输入框获取焦点时显示提示信息,隐藏其他错误信息--------*/
            $(this).siblings("label.loginError").hide();
            /* ADD END   BY geshuo 20151205: 输入框获取焦点时显示提示信息--------*/
        });

        <!--光标离开的校验-->
        $("#userName").blur(function () {
            $(this).siblings("p.loginError").hide();
            /* $(this).siblings(".loginError").show(); */

            var userName = $("#userName").val();
            var qunaju = $("#qunaju");
            <!--校验名称格式-->
            /*修改正则表达式 by 邸磊 原表达式： ^[^_][A-Za-z]*[a-z0-9_]*$ */
            var ragex = /^[a-zA-Z0-9_]*$/;
            if (userName.length > 0) {
                if (userName.length <= 5 || !ragex.test(userName)) {
                    $("#userw").show();
                    $("#userw").siblings("p.loginError").hide();
                    $("#userName").addClass("redborder");
                    qunaju.val("youcuo");
                    return false;
                }
            }
            <!--校验名称唯一-->
            var zouma = false;

            callService("/validateUser.ajax", {username: userName}, {
                type: 'GET',
                async: false,
                timeout: 1000,
                callSuccess: function (str) {
                    namespot = 0;
                    $("#userName").siblings("p.loginError").hide();
                    $("#userName").removeClass("redborder");
                    if (!str) {
                        namespot = 1;
                        $("#userh").siblings("p.loginError").hide();
                        $("#userh").show();
                        $("#userw").hide();
                        $("#userName").addClass("redborder");
                        qunaju.val("youcuo");
                        zouma = false;
                    } else {
                        zouma = true;
                    }

                }
            })

            if (zouma)
                qunaju.val("meicuo");

        });


        $("#password1").blur(function () {
            $(this).siblings("p.loginError").hide();
            var qunaju = $("#qunaju");
            var userName = $("#userName").val();
            var password = $("#password1").val();
            /* var ragex = /^[^_][A-Za-z]*[a-z0-9_]*$/; */
            var ragex =	/^[a-zA-Z0-9_]*$/;
            if (password.length > 0) {
                /* password.indexOf(" ")>-1|| */
                if (!ragex.test(password) || password == userName || password.length < 6) {
                    $("#pwdem").hide()
                    $("#pwdww").show()
                    $("#pwdj").hide();
                    $("#password").addClass("redborder")
                    qunaju.val("youcuo");
                    return false;
                }


            }
            pwStrengthFun(password);
            qunaju.val("meicuo");
        });


        $("#confirmPassword").blur(function () {
            var qunaju = $("#qunaju")
            var confirmPassword = $("#confirmPassword").val();
            var password = $("#password1").val();
            if (confirmPassword.length != password.length || confirmPassword != password) {
                $("#pwde").siblings(".loginError").hide();
                $("#pwdt").siblings(".loginError").show();
                $("#pwdt").siblings("p.loginError").hide();
                $("#confirmPassword").addClass("redborder");
                qunaju.val("youcuo");
                return false;
            }else{
                $("#confirmPassword").removeClass("redborder");
            }
            qunaju.val("meicuo");
        });


        $("#mobile").blur(function () {
            var qunaju = $("#qunaju")
            var mobile = $("#mobile").val();
            var ragex = /^(((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1})|(17[0-9]{1})|(14[0-9]{1}))+\d{8})$/;
            if (mobile.length > 0) {
                if (!ragex.test(mobile)) {
                    $("#ms3").hide();
                    $("#m1").show();
                    $("#m2").hide();
                    $("#mobile").siblings("p.loginError").hide();
                    $("#mobile").addClass("redborder");
                    qunaju.val("youcuo");
                    return false;
                }
                var zoubu = false;

                callService("/validateMobile.ajax", {mobile: mobile}, {
                    type: 'GET',
                    async: false,
                    callSuccess: function (str) {
                        if (!str) {
                            $("#ms3").hide();
                            $("#m1").hide();
                            $("#m2").show();
                            $("#mobile").siblings("p.loginError").hide();
                            $("#mobile").addClass("redborder");
                            qunaju.val("youcuo");
                            zoubu = false;
                            bushiba = false;
                        }
                        else {
                            bushiba = true;
                            zoubu = true;
                        }
                    },
                    timeout: 1000
                })
                if (zoubu)
                    qunaju.val("meicuo");
            }
        });

        $("#captcha").blur(function () {
            var qunaju = $("#qunaju")
            var captcha = $("#captcha").val();
            var password = $("#password1").val();
            if(!captcha){
                $("#captchaNull").css("display","block");
                $("#captcha").addClass("redborder");
            }else {
                callService("/fsajax/toRegisterCaptchaCk.ajax", {captcha: captcha}, {
                    type: 'POST',
                    async: false,
                    callSuccess: function (str) {
                        if ("fail" == str) {
                            $("#captchaWrong").css("display","block");
                            $("#captcha").addClass("redborder");
                            qunaju.val("youcuo");
                        }  else if ("success" == str) {
                            $("#captchaWrong").css("display","none");
                            $("#captcha").removeClass("redborder");
                            qunaju.val("meicuo");
                        } else{
                        }
                    },
                    timeout: 1000
                })
            }


        });

        <!--点击提交时的校验-->

    })

    function cc() {
