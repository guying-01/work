                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#userName").blur();
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#password1").blur();

                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#confirmPassword").blur();
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#mobile").blur();
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#captcha").blur();
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                }
                $("#authCode").blur();
                if (qj.val() == "youcuo") {
                    $.dm.unblock($target);//解除锁定
                    return false;
                } else if (qj.val() == "meicuo") {
                    if ($("#tongyi").attr("checked") != "checked") {
                        $.dm.unblock($target);//解除锁定
                        jsErrAlert("请同意《用户注册协议》");
