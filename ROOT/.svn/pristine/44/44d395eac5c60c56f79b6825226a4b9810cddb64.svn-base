    function notNull() {
        var qunaju = $("#qunaju");
        var userName = $("#userName");
        <!--校验名称为空-->
        if (userName.val() == null || userName.val() == "") {
            $("#usere").show();
            $("#userw").hide();
            $("#usere").siblings("p.loginError").hide();
            $("#userName").addClass("redborder");
            qunaju.val("youcuo");
        }

        var password = $("#password1");
        if (password.val().length == 0) {
            $("#pwdww").hide();
            $("#pwdem").show();
            $("#pwdem").siblings("p.loginError").hide();
            $("#pwdj").hide();
            $("#password").addClass("redborder");
            qunaju.val("youcuo");
        }

        var confirmPassword = $("#confirmPassword");
        if (confirmPassword.val().length == 0) {
            $("#pwde").siblings("p.loginError").hide();
            $("#pwde").siblings("label.loginError").show();

            $("#confirmPassword").addClass("redborder");
            qunaju.val("youcuo");
        }

        var mobile = $("#mobile");
        if (mobile.val().length == 0) {
            $("#ms3").show();
            $("#m1").hide();
            $("#m2").hide();

            $("#mobile").addClass("redborder");
            qunaju.val("youcuo");
        }

        var authCode = $("#authCode");
        if (authCode.val().length == 0) {
            $("#a1").hide();
            $("#a2").show();
            $("#authCode").addClass("redborder");
            qunaju.val("youcuo");
        }

        var buyerName = $("#buyerName").val();
        $("#buyerName").val(buyerName.replace(/(^\s*)|(\s*$)/g,"") );
        if ( $("#buyerName").val().length == 0) {
            $("#buyerNameE1").show();
            $("#buyerName").removeClass("redborder");
            $("#buyerName").addClass("redborder");
            qunaju.val("youcuo");
        }
        var buyerLocation = $("#buyerLocation").val();
