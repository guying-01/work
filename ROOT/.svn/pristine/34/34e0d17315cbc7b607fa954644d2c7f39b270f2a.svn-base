*{
    box-sizing: border-box;
    position: relative;
}
.container-gy{
    background: #fd8222;
    color: #fff;
    overflow: hidden;
    padding: 0 0.1rem;
}
.container-gy .time{
    width: 100%;
    padding-top: 0.1rem;
    text-align: center;
    color: #fff;
    font-size: 0.3rem;
    letter-spacing: 0.05rem;
}
.container-gy .title{
    width: 100%;
    padding: 0.1rem 0;
    text-align: center;
    color: #fff;
    font-size: 0.27rem;
    letter-spacing: 0.05rem;

}
.promotion-common-content{
    overflow: hidden;
    padding-top: 0.2rem;
    background: #faae00;
}
.rangeAds{
    overflow: hidden;
}
.everyAds{
    width: 48%;
    float: left;
    margin-left: 1%;
    margin-right: 1%;
    padding: 0.1rem;
    background: #fff;
    color: #000;
    margin-bottom: 0.2rem;
}
.promotion-box-gy{
    overflow: hidden;
}
.everyAds .itemPic{
    width: 100%;
    height: 2.7rem;
}
.everyAds .itemPic img{
    width: 100%;
    height: 100%;
}
.itemName,.itemDescription{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding-bottom: 0.05rem;
    line-height: 0.45rem;
    height: 0.45rem;
}
.itemName{
    padding-top: 0.05rem;
}
.itemPrice span{
    color: #afaeae;
}
.wrap{
    overflow: hidden;
}
.wrap .itemPrice{
    line-height: 0.3rem;
}
.priceBox{
    white-space: nowrap;
}
.wrap .buy{
    padding: 0.02rem;
    background: #E11020;
    color: #fff;
    width: 100%;
    font-size: 0.24rem;
    text-align: center;
    float: right;
    line-height: 0.6rem;
    margin-top: 0.1rem;
    letter-spacing: 2px;
    border-radius: 0.1rem;
}
.everyAds a{
    position: absolute;
    left: 0;
    top:0;
    width: 100%;
    height: 100%;
    z-index: 1;
}
.titleSelect{
    width: 100%;
    border-bottom: 0.05rem solid #fff;
    overflow: hidden;
}
.titleSelect span{
    width: 50%;
    box-sizing: border-box;
    font-size: 0.3rem;
    text-align: center;
    padding: 0.1rem 0;
    float: left;
}
.winningBox{
    overflow: hidden;
    padding-bottom: 0.1rem;
    border-bottom: 1px solid #fff;
}
.winningGoods,.winningStore{
    width: 49%;
    box-sizing: border-box;
    height: 4.25rem;
    font-size: 0.25rem;
    float: left;
    margin-right: 1%;
}
.winningStore li{
    text-align: center;
    padding: 0.05rem;
    box-sizing: border-box;
    border-bottom: 1px solid #fff;
    height: 0.4rem;
}
.showMore .iconMore{
    height: 100%;
    width: 20%;
    float: left;
    background:#fff url("../img/pie/pie-click.png") no-repeat center center;
    background-size: contain;
}
.showMore{
    height: 0.6rem;
    line-height: 0.6rem;
    margin-top: 0.4rem;
    border: 1px solid #fff;
    border-radius: 0.05rem;
}
.showMore .word{
    width: 80%;
    float: left;
    text-align: center;
}
.promotion-hb-fill{
    width: 100%;
    height: 0.3rem;
}
.see-details{
    background: #b1181a;
    color: #fff;
    width: 100%;
    text-align: center;
    padding: 0.1rem;
    box-sizing: border-box;
    font-size: 0.25rem;
    margin-top: 0.2rem;
}
.promotion-common-content a:active{
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.ff_list_search_box {
    height: 0.73rem;
    position: relative;
}
.orderMore[canClick='true']{
    width: 30%;
    margin: 0.3rem auto;
    height: 0.7rem;
    background: #b1181a;
    color: #fff;
    text-align: center;
    line-height: 0.7rem;
    border-radius: 0.1rem;
    font-size: 0.24rem;
}
.orderMore[canClick='false']{
    width: 100%;
    background: none;
    text-align: center;
    font-size: 0.24rem;
    padding: 0.3rem 0;
    color: #fff;
}